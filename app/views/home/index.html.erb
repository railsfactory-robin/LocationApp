  </div>  
    <div class="container"> 
      <div class="row header-text">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12"></div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 text-center">
          <h2>THE ULTIMATE COLLECTION OF FREE PSD TEMPLATES!!</h2>
          <!-- <button class="btn btn-download" data-toggle="modal" data-target="#myModal">DOWNLOAD</button> -->
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12"></div>
      </div>
       <div class="row location-search">
         <div class="col-sm-3 col-sm-offset-1 form-group">
           <input value="data.location" ng-model="data.location" class="form-control" type="text" name="location" placeholder="Enter City" id="location"/>
         </div>
         <div class="col-sm-3 form-group">
        <select ng-init="getCategoryList()" class="form-control" ng-model="data.category"  ng-change="locationData(data);">
           <option value="">---Please select---</option>
           <option ng-repeat="cat in category" value="{{cat.id}}">{{cat.name}}</option>
         </select>
         <%#= select_tag "category", options_for_select(get_category_list),{prompt: "--select county--",class: "form-control"}%>
        
          </div>
         <div class="col-sm-3 form-group">
          <input type="text" ng-model="data.keyword" name="search" class="form-control" placeholder="Search Key" />
         </div>
         <div class="col-sm-2">
          <button class="btn btn-download" ng-click="locationData(data)">Search</button>
         </div>
      </div>
    </div>
    
  </header>
  <ng-view></ng-view>
